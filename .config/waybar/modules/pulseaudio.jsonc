{
  "pulseaudio": {
    "format": "{icon} {volume}%",
    "format-bluetooth": "{volume}% {icon}",
    "format-icons": {
      "headphones": "",
      "handsfree": "",
      "headset": "",
      "phone": "",
      "portable": "",
      "car": "",
      "default": ["", ""]
    },
    "format-muted": "{icon} ",
    "on-click": "~/.config/waybar/scripts/switch-sink.mjs",
    "on-click-middle": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle;",
    "on-click-right": "pwvucontrol"
  }
}
